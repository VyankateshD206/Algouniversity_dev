# COPY THIS FILE TO .env AND FILL IN YOUR ACTUAL CREDENTIALS

# MongoDB Configuration - Replace with your actual MongoDB Atlas credentials
MONGO_URI=mongodb+srv://your_username:your_password@your_cluster.xxxxx.mongodb.net/byteforge?retryWrites=true&w=majority

# JWT Configuration - Generate a random secret key (at least 32 characters)
JWT_SECRET_KEY=your_very_long_and_random_secret_key_here_make_it_at_least_32_characters
JWT_EXPIRES_IN=7d

# Server Configuration
PORT=5000
NODE_ENV=development

# CORS Configuration
FRONTEND_URL=http://localhost:5173
COMPILER_URL=http://localhost:5008

# Redis Configuration (optional for caching - leave as default for local Redis)
REDIS_URL=redis://localhost:6379

# RabbitMQ Configuration (required for job queue - leave as default for local RabbitMQ)
RABBITMQ_URL=amqp://localhost

# Gemini AI Configuration (optional for AI features - get from Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here
